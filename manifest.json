{
    "manifest_version": 2,
    "name": "Priberam Search",
    "version": "1.3.1",
    "description": "Searches dictionary entries in priberam.",
    "icons": {
        "128": "images/icons/transparent_icon.png"
    },
    "browser_action": {
        "default_icon": "images/icons/transparent_icon.png",
        "default_popup": "html/popup.html"
    },
    "permissions": [
        "tabs",
        "http://*/*",
        "https://*/*"
    ]
}
