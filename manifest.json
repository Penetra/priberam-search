{
    "manifest_version": 2,
    "name": "Priberam Search",
    "version": "1.3.1",
    "description": "Searches dictionary entries in priberam.",
    "icons": {
        "128": "images/icons/simbolo.jpg"
    },
    "browser_action": {
        "default_icon": "images/icons/simbolo.jpg",
        "default_popup": "html/popup.html"
    },
    "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
    ]
}
